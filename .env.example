# Docker environment variables
# Copy this file to .env and fill in your own values

# Symfony application secret - generate a random string for production
# You can generate one using: openssl rand -hex 32
APP_SECRET=change_this_to_a_random_secret_in_production

# JWT passphrase for signing tokens - generate a strong passphrase for production
JWT_PASSPHRASE=change_this_to_a_strong_passphrase
